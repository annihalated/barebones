<html lang=en>
<head>
  <title>nihalsahu.net</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta property="og:title" content="nihalsahu.net">
  <meta property="og:site-name" content="nihalsahu.net">
  <meta property="og:url" content="https://nihalsahu.net">
  <meta property="og:description" content="Nihal Sahu's personal website.">
  <meta name="twitter:card" content="summary">
  <meta name="twitter:site" content="@annihalated">
  <link rel="canonical" href="https://nihalsahu.net">
  <link rel="stylesheet" href="/style.css">
</head>
<body>
  <h1 id=site-title>{{ title }}</h1>
  <div id="home">
  {{ content | safe }}
  <h2>Posts</h2>
  <ul>
  {%- for post in collections.post -%}
  <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
  {%- endfor -%}
  </ul>
  </div>
  {% include "./partials/footer.njk" %}
</body>
</html>
